{
  "name": "nestjs-boilerplate",
  "version": "0.1.3",
  "license": "MIT",
  "author": {
    "name": "Harlan Cleiton",
    "email": "harlancleiton@gmail.com",
    "url": "https://github.com/harlancleiton"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "cross-env NODE_ENV=development ts-node-dev --inspect --clear --transpile-only --no-notify -r tsconfig-paths/register src/main.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "postinstall": "husky install",
    "test": "jest",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.config-e2e.js --runInBand",
    "release": "standard-version",
    "prepare": "husky install",
    "typeorm": "cross-env NODE_ENV=development ts-node-dev --no-notify --clear -r tsconfig-paths/register node_modules/typeorm/cli",
    "typeorm:prod": "cross-env NODE_ENV=production ts-node-dev --no-notify --clear -r tsconfig-paths/register node_modules/typeorm/cli"
  },
  "dependencies": {
    "@bull-board/api": "^3.6.0",
    "@bull-board/express": "^3.6.0",
    "@nestjs-modules/mailer": "1.5.1",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^8.0.11",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.11",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^9.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.11",
    "@nestjs/typeorm": "^8.0.2",
    "apollo-server-express": "^3.3.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.29.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.6.1",
    "graphql-tools": "^8.2.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "mongoose": "^5.12.13",
    "nodemailer": "^6.7.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "typeorm": "^0.2.38",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.11",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.15.5",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.5.3",
    "fishery": "^1.4.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
