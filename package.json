{
  "name": "nestjs-boilerplate",
  "version": "0.1.3",
  "license": "MIT",
  "author": {
    "name": "Harlan Cleiton",
    "email": "harlancleiton@gmail.com",
    "url": "https://github.com/harlancleiton"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "cross-env NODE_ENV=development ts-node-dev --inspect --clear --transpile-only --no-notify -r tsconfig-paths/register src/main.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "postinstall": "husky install",
    "test": "jest",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.config-e2e.js --runInBand",
    "release": "standard-version",
    "prepare": "husky install",
    "typeorm": "cross-env NODE_ENV=development ts-node-dev --no-notify --clear -r tsconfig-paths/register node_modules/typeorm/cli",
    "typeorm:prod": "cross-env NODE_ENV=production ts-node-dev --no-notify --clear -r tsconfig-paths/register node_modules/typeorm/cli"
  },
  "dependencies": {
    "@bull-board/api": "^3.2.8",
    "@bull-board/express": "^3.2.8",
    "@nestjs-modules/mailer": "1.5.1",
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/event-emitter": "^0.0.3",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.25.1",
    "bcryptjs": "^2.4.3",
    "bull": "^3.22.8",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "mongoose": "^5.12.13",
    "nodemailer": "^6.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.15.1",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.5.3",
    "fishery": "^1.3.1",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "standard-version": "^9.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  }
}
